package org.yueqian.Tes1;

public class Tse_1 {
	public static void main (String []args){
		xues xues = new xues();
		sud sud = new sud();
		System.out.println(xues.xusr());
	}
}

class sud{
	public void sudlt(String xues) {
		System.out.print("上厕所");
	}
}

class xues{
	public String xues;
	public String  xusr(){
		synchronized (this) {
			return "张三";
		}
	}
	public String xusr1(){
		synchronized (this) {
			return "李四";
		}
	}
	
}